  _paragraphs:
  - Name: Mind
    Body: "\u8FD9\u4E9B\u7167\u7247\u4E00\u6837\u7684\u4E1C\u897F\u662F\u4EC0\u4E48\uFF1F"
    ObjectName: 
    Skip: 0
    Dialog: 1
    Control: 1
  - Name: EMO
    Body: "\u662F\u5BBF\u4E3B\u7684\u4E00\u4E9B\u8BB0\u5FC6\u788E\u7247\uFF0C\u5B83\u4EEC\u90FD\u662F\u57F9\u517B\u57FA\u7684\u8425\u517B\u6E90\u3002"
    ObjectName: 
    Skip: 0
    Dialog: 1
    Control: 1
  - Name: EMO
    Body: "\u6CA1\u60F3\u5230\u652F\u6491\u8FD9\u4E2A\u8FDD\u7981\u54C1\u7684\u8425\u517B\uFF0C\u5C45\u7136\u662F\u90A3\u4E48\u591A\u5F00\u5FC3\u7684\u77AC\u95F4..."
    ObjectName: 
    Skip: 0
    Dialog: 1
    Control: 1
  - Name: EMO
    Body: "\u5929\u8001\u7237\uFF0C\u8BA9\u6211\u90FD\u6709\u70B9\u6000\u7591\u4E3A\u4EC0\u4E48\u4F1A\u662F\u8FDD\u7981\u54C1\u4E86\uFF01"
    ObjectName: 
    Skip: 0
    Dialog: 1
    Control: 1
  - Name: EMO
    Body: "\u4E0D\u8FC7\u5DF2\u7ECF\u8FC7\u53BB\u592A\u4E45\uFF0C\u6211\u4E5F\u5FD8\u8BB0\u4E3A\u4EC0\u4E48\u8981\u9500\u6BC1\u5B83\u4E86\u3002"
    ObjectName: 
    Skip: 0
    Dialog: 1
    Control: 1
  - Name: Mind
    Body: "\u4F60\u4E0D\u662F\u8BF4\uFF0C\u662F\u56E0\u4E3A\u5BB3\u6015\u5B83\u5F71\u54CD\u5BBF\u4E3B\u7684\u4EBA\u9645\u5173\u7CFB\u5417\uFF1F"
    ObjectName: 
    Skip: 0
    Dialog: 1
    Control: 1
  - Name: Mind
    Body: "\u5927\u6982\u662F\u89C9\u5F97\u8FD9\u79CD\u5FC3\u52A8\u4F1A\u7ED9\u6708\u9999\u5E26\u6765\u56F0\u6270\u5427\u3002"
    ObjectName: 
    Skip: 0
    Dialog: 1
    Control: 1
  - Name: Mind
    Body: "\u4E0D\u8FC7\u5927\u5FC3\u60C5\uFF0C\u65E2\u7136\u8FD9\u4EFD\u57F9\u517B\u57FA\u4E00\u76F4\u5B8C\u597D\u65E0\u635F\u5730\u4FDD\u5B58\u5728\u5230\u73B0\u5728......"
    ObjectName: 
    Skip: 0
    Dialog: 1
    Control: 1
  - Name: Mind
    Body: "\u90A3\u4E48\u5BBF\u4E3B\u5373\u4F7F\u60F3\u5426\u5B9A\u8FD9\u4EFD\u5FC3\u52A8\uFF0C\u4E5F\u4E00\u5B9A\u662F\u4E0D\u613F\u610F\u653E\u5F03\u7684\u5427\uFF1F"
    ObjectName: 
    Skip: 0
    Dialog: 1
    Control: 1
  - Name: Mind
    Body: "\u4F60\u89C9\u5F97\u662F\u4E0D\u662F<color=E82372>\u6709\u5FC5\u8981</color>\u5C06\u4ED6\u7559\u4E0B\u5462\uFF1F"
    ObjectName: 
    Skip: 0
    Dialog: 1
    Control: 1
  - Name: EMO
    Body: "\u55EF......\u4F60\u8BF4\u5F97\u5BF9\u3002"
    ObjectName: 
    Skip: 0
    Dialog: 1
    Control: 1
  - Name: EMO
    Body: "\u597D\u5427\u9EA6\u5FB7\u3002\u4E0D\u8FC7\u6211\u4EEC\u5F97\u6084\u6084\u7684\u3002\u4E0D\u8981\u544A\u8BC9\u5176\u4ED6EGO\u54E6\uFF01"
    ObjectName: 
    Skip: 0
    Dialog: 1
    Control: 1
